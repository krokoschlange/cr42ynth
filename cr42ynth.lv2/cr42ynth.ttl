@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .

<https://github.com/krokoschlange/cr42ynth#ui>
	a ui:X11UI ;
	lv2:binary <bin/cr42ynth_ui.so> .

<https://github.com/krokoschlange/cr42ynth>
	a lv2:Plugin , lv2:InstrumentPlugin ;
	doap:name "CR42Ynth" ;
	doap:license <https://opensource.org/licenses/BSD-3-Clause> ;
	lv2:binary <bin/cr42ynth.so> ;
	lv2:project <http://lv2plug.in/ns/lv2> ;
	lv2:requiredFeature urid:map ;
	
	ui:ui <https://github.com/krokoschlange/cr42ynth#ui> ;
	
	lv2:port [
		a lv2:InputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent ;
		lv2:designation lv2:control ;
		lv2:index 0 ;
		lv2:symbol "midi_in" ;
		lv2:name "Midi In" ;
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 1 ;
		lv2:symbol "out_l" ;
		lv2:name "Left" ;
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 2 ;
		lv2:symbol "out_r" ;
		lv2:name "Right" ;
	] , [
		a lv2:InputPort ,
			atom:atomPort ;
		atom:bufferType atom:Sound ;
		lv2:index 3 ;
		lv2:symbol "wavetable" ;
		lv2:name "WT" ;
	] , [
		a lv2:InputPort ,
			atom:atomPort ;
		atom:bufferType atom:Sound ;
		lv2:index 4 ;
		lv2:symbol "envelope" ;
		lv2:name "ENV" ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "wtread" ;
		lv2:name "WTread" ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "envread" ;
		lv2:name "ENVread" ;
	].